
@{
    ViewData["Title"] = "AccountStatementError";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <script src='https://kit.fontawesome.com/a076d05399.js'></script>
    <style>
        .body {
            text-align: center;
        }

        #tb {
            text-align: center;
        }

        #hd {
            text-align: right;
        }

        #unl {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            background-color: #333;
        }

        #list {
            float: left;
        }

        li a {
            display: block;
            color: white;
            text-align: center;
            padding: 14px 16px;
            text-decoration: none;
        }

        #ab {
            display: block;
            color: white;
            text-align: center;
            padding: 18px 16px;
            text-decoration: none;
        }

        li a:hover:not(.active) {
            background-color: #111;
        }

        .active {
            background-color: #4CAF50;
        }

        #abcd {
            float: right;
            color: white;
        }
    </style>
</head>
<body>
    <header>
        <ul id="unl">
            <li id="list"><span id="ab" class="active">Account Statement</span></li>
            <li id="list"><span id="abcd" onclick="window.print()">Print<i class='fas fa-print' style='font-size:30px;padding-top:12px;color:#b3f7ae'></i></span></li>
            <li id="list"><a asp-action="Index">Home<i class='fas fa-home' style='font-size:30px;color:#b3f7ae'></i></a></li>
        </ul>
    </header>
    <h1>Check the Account ID or Date entered.</h1>
    </body>

